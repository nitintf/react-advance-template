    "graphql-ws": "5.11.2",
    "helmet": "6.0.1",
    "localenv": "0.2.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^3.1.4",
    "react-router-dom": "6.8.0",
    "winston": "3.8.2"
