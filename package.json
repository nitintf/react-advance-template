{
	"name": "react-starter",
	"version": "1.0.0",
	"description": "React Starter with Webpack",
	"main": "index.tsx",
	"engines": {
		"node": "^16.17.1"
	},
	"scripts": {
		"clean": "rimraf dist/*",
		"dev": "yarn prebuild && wp --env dev",
		"build": "cross-env NODE_ENV=production wp --env prod",
		"prebuild": "yarn clean && installed-check",
		"format": "yarn format:base -- \"**/*.{ts,tsx,js,jsx,yml,json,md}\"",
		"format:base": "npx prettier",
		"format:fix": "yarn format -- --write",
		"lint": "yarn lint:base -- \"**/*.{ts,tsx,js,jsx}\"",
		"lint:base": "yarn tsc && npx eslint",
		"lint:fix": "yarn lint -- --fix",
		"tsc": "tsc",
		"precommit": "lint-staged",
		"test:nocoverage": "jest --coverage=false",
		"test:debug": "node --inspect node_modules/.bin/jest --runInBand --coverage=false",
		"test-watch": "jest --config jest.config.js --watch --coverage=false",
		"storybook": "start-storybook -p 6006"
	},
	"keywords": [
		"react",
		"webpack",
		"redux"
	],
	"author": "Nitin Panwar",
	"license": "MIT",
	"dependencies": {
		"@reduxjs/toolkit": "^1.8.5",
		"@sentry/browser": "^7.14.1",
		"@storybook/addon-storyshots": "^6.5.12",
		"@storybook/react": "^6.5.12",
		"@testing-library/react": "^13.4.0",
		"@types/jest": "^29.1.1",
		"@types/node": "^18.8.2",
		"@welldone-software/why-did-you-render": "^7.0.1",
		"assert": "^2.0.0",
		"axios": "^1.0.0",
		"change-case": "^4.1.2",
		"chroma-js": "^2.4.2",
		"ejs": "^3.1.8",
		"history": "^5.3.0",
		"node-sass": "^7.0.3",
		"normalize.css": "^8.0.1",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-helmet": "^6.1.0",
		"react-redux": "^8.0.4",
		"react-router": "^6.4.1",
		"react-router-dom": "^6.4.1",
